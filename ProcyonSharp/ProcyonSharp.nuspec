<?xml version="1.0" encoding="utf-8"?>

<package>
  <metadata>
    <id>ProcyonSharp</id>
    <version>1.0.3</version>
    <title>ProcyonSharp</title>
    <authors>Addison Schuhardt</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/aschuhardt/ProcyonSharp</projectUrl>
    <description>NETStandard bindings for my personal roguelike engine.  Includes built-in support for configurable input mapping.</description>
    <releaseNotes>Bug fixes and quality of life improvements</releaseNotes>
    <copyright>Copyright 2021 Addison Schuhardt</copyright>
    <tags>roguelike engine procyon gamedev tui</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.1">
        <dependency id="ProcyonSharp.Attributes" version="1.0.0" />
        <dependency id="ProcyonSharp.Input" version="1.0.0" />
        <dependency id="ProcyonSharp.Bindings" version="1.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\netstandard2.1\ProcyonSharp.dll" target="lib\netstandard2.1\" />
    <file src="runtimes\**" target="runtimes\" />
  </files>
</package>
